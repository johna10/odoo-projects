<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <!--List view-->
    <record id="deligation_keyboard_list_view" model="ir.ui.view">
        <field name="name">list</field>
        <field name="model">delegation.keyboard</field>
        <field name="arch" type="xml">
            <list string="DeligationkeyboardList">
                <field name="layout"/>
            </list>
        </field>
    </record>

    <!--Form View-->
    <record id="deligation_keyboard_form_view" model="ir.ui.view">
        <field name="name">form</field>
        <field name="model">delegation.keyboard</field>
        <field name="arch" type="xml">
            <form string="DeligationkeyboardForm">
                <sheet>
                    <group>
                        <group>
                            <field name="layout"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!--Action-->
    <record id="deligation_keyboard_model_action" model="ir.actions.act_window">
         <field name="name">Deliation keyboard</field>
         <field name="res_model">delegation.keyboard</field>
         <field name="view_mode">list,form</field>
    </record>

</odoo>